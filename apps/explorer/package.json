        "tailwindcss": "^3.2.4",
        "typescript": "^4.8.3",
        "vite": "^4.0.0",
        "vite-plugin-svgr": "^2.4.0",
        "vitest": "^0.25.7",
        "wait-on": "^6.0.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
